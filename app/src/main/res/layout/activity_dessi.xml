<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
    app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
    tools:context=".Activities.DessiActivity">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="29dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton32"
        android:layout_toEndOf="@+id/imageButton32" />

    <ImageButton
        android:id="@+id/imageButton32"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="21dp"
        app:srcCompat="@drawable/astronaut" />

    <ImageButton
        android:id="@+id/imageButton31"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_below="@+id/imageButton26"
        android:layout_alignStart="@+id/imageButton26"
        android:layout_marginTop="24dp" />

    <ImageButton
        android:id="@+id/imageButton30"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton29"
        android:layout_alignStart="@+id/imageButton25" />

    <ImageButton
        android:id="@+id/imageButton29"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton28"
        android:layout_toStartOf="@+id/imageButton25" />

    <ImageButton
        android:id="@+id/imageButton28"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton27"
        android:layout_alignStart="@+id/imageButton23" />

    <ImageButton
        android:id="@+id/imageButton27"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton26"
        android:layout_alignStart="@+id/imageButton" />

    <ImageButton
        android:id="@+id/imageButton26"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_below="@+id/imageButton32"
        android:layout_alignStart="@+id/imageButton32"
        android:layout_marginTop="18dp" />

    <ImageButton
        android:id="@+id/imageButton25"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="17dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton24"
        android:layout_toEndOf="@+id/imageButton24" />

    <ImageButton
        android:id="@+id/imageButton24"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton23"
        android:layout_toEndOf="@+id/imageButton23" />

    <ImageButton
        android:id="@+id/imageButton23"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="28dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/my_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        android:elevation="4dp"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Dark"/>


    <include layout="@layout/profile" android:id="@+id/profile"
        android:layout_below="@+id/my_toolbar"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"></include>


</RelativeLayout>
