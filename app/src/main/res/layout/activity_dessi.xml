<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
    app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
    tools:context=".Activities.DessiActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/profile">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/imageButton1"
                android:layout_alignStart="@+id/imageButton1"
                android:layout_below="@+id/imageButton1"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"                android:layout_below="@+id/imageButton4"
                android:layout_alignStart="@+id/imageButton4"
                android:layout_alignEnd="@+id/imageButton4" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"                android:layout_below="@+id/imageButton3"
                android:layout_alignStart="@+id/imageButton3"
                android:layout_alignEnd="@+id/imageButton3" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"
                android:layout_below="@+id/imageButton6"
                android:layout_alignStart="@+id/imageButton6"
                android:layout_alignEnd="@+id/imageButton6" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"
                android:layout_below="@+id/imageButton9"
                android:layout_alignStart="@+id/imageButton9"
                android:layout_alignEnd="@+id/imageButton9" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/imageButton11"
                android:layout_alignStart="@+id/imageButton11"
                android:layout_below="@+id/imageButton11"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"
                />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"
                android:layout_centerVertical="true"
                android:layout_alignStart="@+id/imageButton9"
                android:layout_alignEnd="@+id/imageButton9" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"
                android:layout_alignBaseline="@+id/textView8"
                android:layout_alignBottom="@+id/textView8"
                android:layout_alignEnd="@+id/imageButton2"
                android:layout_alignStart="@+id/imageButton2" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"
                android:layout_centerVertical="true"
                android:layout_alignStart="@+id/imageButton6"
                android:layout_alignEnd="@+id/imageButton6" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100%"
                android:textColor="@color/colorPrimary"
                android:textAlignment="center"
                android:layout_below="@+id/imageButton7"
                android:layout_alignStart="@+id/imageButton7"
                android:layout_alignEnd="@+id/imageButton7" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100%"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"
                android:layout_below="@+id/imageButton10"
                android:layout_alignStart="@+id/imageButton10"
                android:layout_alignEnd="@+id/imageButton10" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_alignStart="@+id/imageButton9"
                android:layout_alignTop="@+id/imageButton3"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_alignStart="@+id/imageButton7"
                android:layout_alignTop="@+id/imageButton11"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginTop="39dp"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation"
                android:layout_below="@+id/imageButton1"
                android:layout_alignStart="@+id/imageButton1" />

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_alignStart="@+id/imageButton2"
                android:layout_alignTop="@+id/imageButton6"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation" />

            <ImageButton
                android:id="@+id/imageButton8"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_alignStart="@+id/imageButton3"
                android:layout_alignTop="@+id/imageButton7"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation" />

            <ImageButton
                android:id="@+id/imageButton9"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation"
                android:layout_alignTop="@+id/imageButton7"
                android:layout_alignStart="@+id/imageButton10" />

            <ImageButton
                android:id="@+id/imageButton10"
                android:layout_width="75dp"
                android:layout_height="75dp"

                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation"
                android:layout_alignTop="@+id/imageButton2"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="36dp" />

            <ImageButton
                android:id="@+id/imageButton11"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginTop="40dp"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation"

                android:layout_below="@+id/imageButton6"
                android:layout_alignStart="@+id/imageButton6" />

            <ImageButton
                android:id="@+id/imageButton1"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginTop="19dp"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:layout_marginStart="37dp"
               />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_alignTop="@+id/imageButton1"
                android:layout_centerHorizontal="true"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/spacestation"
                android:clickable="false"/>
        </RelativeLayout>
    </ScrollView>

    <android.support.v7.widget.Toolbar
        android:id="@+id/my_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        android:elevation="4dp"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Dark"/>


    <include layout="@layout/profile" android:id="@+id/profile"
        android:layout_below="@+id/my_toolbar"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"></include>


</RelativeLayout>
