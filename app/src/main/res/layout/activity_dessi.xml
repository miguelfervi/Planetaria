<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
    app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
    tools:context=".Activities.DessiActivity">

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="29dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton1"
        android:layout_toEndOf="@+id/imageButton1" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="22dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_below="@+id/profile"
        android:layout_alignParentStart="true"
        android:layout_marginTop="35dp" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/imageButton6"
        android:layout_below="@+id/imageButton6"
        android:layout_marginTop="24dp"
        app:srcCompat="@drawable/astronaut" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/imageButton5"
        android:layout_alignTop="@+id/imageButton9"
        app:srcCompat="@drawable/astronaut" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton8"
        android:layout_toStartOf="@+id/imageButton5" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton7"
        android:layout_alignStart="@+id/imageButton3" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_alignStart="@+id/imageButton2" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_below="@+id/imageButton1"
        android:layout_alignStart="@+id/imageButton1"
        android:layout_marginTop="18dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="17dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_toEndOf="@+id/imageButton4" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_toEndOf="@+id/imageButton3" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="28dp"
        app:srcCompat="@drawable/astronaut"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_toEndOf="@+id/imageButton2" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/my_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        android:elevation="4dp"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Dark"/>


    <include layout="@layout/profile" android:id="@+id/profile"
        android:layout_below="@+id/my_toolbar"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"></include>


</RelativeLayout>
